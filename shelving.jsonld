                {
                    "type": "object",
                    "required": [
                        "error",
                        "result"
                    ],
                    "properties": {
                        "result": {
                            "type": "null"
                        },
                        "error": {
                            "type": "object",
                            "required": [
                                "errorType",
                                "message"
                            ],
                            "properties": {
                                "errorType": {
                                    "type": "string",
                                    "enum": [
                                        "ShelfIdNotPresent",
                                        "ShelfIdNotRemoved"
                                    ]
                                },
                                "message": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            ]
        }
    },
