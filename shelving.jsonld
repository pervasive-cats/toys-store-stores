            ],
           "properties": {
               "shelfId": {
                    "type": "integer",
                    "minimum": 0 
               }, 
               "itemsRowId": {
                    "type": "integer",
                    "minimum": 0 
               }
           } 
        },
        "output": {
            "oneOf": [
                {
                    "type": "object",
                    "required": [
                        "error",
                        "result"
                    ],
                    "properties": {
                        "result": {
                            "type": "integer",
