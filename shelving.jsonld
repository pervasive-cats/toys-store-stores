                    ],
                    "properties": {
                        "result": {
                            "type": "null"
                        },
                        "error": {
                            "type": "object",
                            "required": [
                                "errorType",
                                "message"
                            ],
                            "properties": {
                                "errorType": {
                                    "type": "string",
                                    "enum": [
                                        "ShelfIdNotPresent",
                                        "ItemsRowIdAlreadyPresent",
                                        "ItemsRowIdNotAdded"
                                    ]
                                },
                                "message": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            ]
        }
    },
    "removeItemsRow": {
        "title": "removeItemsRow",
        "description": "Remove a row of items from this shelving.",
        "forms": [
