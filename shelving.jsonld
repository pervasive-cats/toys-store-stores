        {
          "op": [
            "subscribeevent"
          ],
          "href": "io.github.pervasivecats:shelving-{storeId}-{shelvingGroupId}-{id}/messages/outbox/catalogItemLiftingRegistered",
          "contentType": "application/json"
        }
      ],
      "data": {
        "type": "object",
        "required": [
          "shelfId",
          "itemsRowId"
        ],
        "properties": {
          "shelfId": {
            "type": "integer",
            "minimum": 0
          },
          "itemsRowId": {
            "type": "integer",
            "minimum": 0
          }
        }
      }
